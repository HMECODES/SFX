function SlowKill(func)
    _G.slowKill = func

        while _G.slowKill do task.wait()
            local Player = game.Players.LocalPlayer
            local character = Player.Character or player.CharacterAdded:Wait()
            local humanoidrootpart = character:WaitForChild("HumanoidRootPart")
            local sword
            game:GetService("Workspace").Mobs:WaitForChild("Noob")
                for i,v in pairs(game:GetService("Workspace").Mobs:GetDescendants()) do
                    if v.Name == "HumanoidRootPart" and _G.slowKill then
                        repeat if _G.slowKill and character then
                            if not character:FindFirstChild("Sword") then
                            sword = character.Humanoid:EquipTool(Player.Backpack:WaitForChild("Sword"))
                            else
                            sword = character:FindFirstChild("Sword")
                            end
                                local dist = (humanoidrootpart.Position - v.Position + Vector3.new(-1, 1, 2)).magnitude
                                local tweenspeed = dist/tonumber(_G.CustomKillSpeed or 30)
                                local info = TweenInfo.new(tonumber(tweenspeed), Enum.EasingStyle.Linear)
                                if _G.slowKill and character:WaitForChild("Sword") then
                                    game:GetService("TweenService"):Create(humanoidrootpart, info, {CFrame = CFrame.new(v.Position + Vector3.new(-1, 1, 3))}):Play()
                                end
                                task.wait()
                            end
                        until _G.slowKill == false or human == nil or v.Parent == nil or v.Parent.Humanoid.Health < 0.1
                    end
                end
            end
end

function FastKill(func)
    _G.fastKill = func

    while _G.fastKill do task.wait()
        local Player = game.Players.LocalPlayer
        local character = Player.Character or player.CharacterAdded:Wait()
        local humanoidrootpart = character:WaitForChild("HumanoidRootPart")
    
        game:GetService("Workspace").Mobs:WaitForChild("Noob")
	    for i,v in pairs(game:GetService("Workspace").Mobs:GetDescendants()) do
            if v.Name == "HumanoidRootPart" and _G.fastKill and character and v then
                if humanoidrootpart then
                    if _G.fastKill and character and Player:FindFirstChild("Sword") and v and v.Parent or not character:FindFirstChild("Sword") then
                        character.Humanoid:EquipTool(Player.Backpack:WaitForChild("Sword")) elseif character:FindFirstChild("Sword") then
                        local Velocity = Instance.new("BodyVelocity")
                        Velocity.Parent = humanoidrootpart
                        Velocity.Velocity = Vector3.new(0, 0, 0)
                        local mobVoc = Velocity:Clone()
                        mobVoc.Parent = v
                            repeat task.wait() if v and v.Parent and character:FindFirstChild("Sword") and _G.fastKill then
                                    local FastKill = coroutine.wrap(function()
                                        humanoidrootpart.CFrame = v.CFrame + Vector3.new(0, -10.1, 0)
                                        for i,v in pairs(v.Parent:GetChildren()) do
                                            if v:IsA("Part") and v and v.Parent and character:FindFirstChild("Sword") and _G.fastKill then
                                                firetouchinterest(character:FindFirstChild("Sword").Handle, v, 0)
                                                firetouchinterest(character:FindFirstChild("Sword").Handle, v, 1)
                                                else
                                            end
                                        end
                                    end)
                                    FastKill()
                                else
                            end
                            until _G.fastKill == false or human == nil or v.Parent == nil or v.Parent.Humanoid.Health < 0.1
                                if humanoidrootpart:FindFirstChild("BodyVelocity") then
                                    for i,v in pairs(humanoidrootpart:GetChildren()) do
                                        if v:IsA("BodyVelocity") then
                                            v:Destroy()
                                        else
                                        end
                                    end
                                end
                            else
                            end
                        end
                        end
                    if humanoidrootpart:FindFirstChild("BodyVelocity") then
                        for i,v in pairs(humanoidrootpart:GetChildren()) do
                            if v:IsA("BodyVelocity") then
                                v:Destroy()
                            else
                            end
                        end
                    end
        end
    end
end

function AutoSwing(func)
    local Player = game.Players.LocalPlayer
    local character = Player.Character or player.CharacterAdded:Wait()
    local humanoidrootpart = character:WaitForChild("HumanoidRootPart")
        _G.autoSwing = func
            while _G.autoSwing do task.wait(.333)
                local sword = character:FindFirstChild("Sword") or character.Humanoid:EquipTool(Player.Backpack:WaitForChild("Sword"))
                    if _G.autoSwing and character:FindFirstChild("Sword") then
                        character:WaitForChild("Sword"):Activate()
                    else
                    end
            end
end

function GodMode(func)
        _G.GodMode = func
            while _G.GodMode do task.wait()
                for i,v in pairs(game:GetService("Workspace").Mobs:GetDescendants()) do
                    if v.Name == "Right Arm" and _G.GodMode then
                        v:Destroy()
                    else
                        if v:IsA("Tool") and _G.GodMode then
                            v:Destroy()
                        else
                    end
                end
        end
end
end

function SwordReach()
    local x = 1
    local y = 1
    local z = 20
    
    for i,v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
        if v.ClassName == "Tool" then
            local reach = Instance.new("SelectionBox", v.Handle)
            reach.Adornee = v.Handle
            v.Handle.Size = Vector3.new(x, y, z)
        end
    end
end

function HideName(func)
    for i,v in pairs(game:GetDescendants()) do
        if v.ClassName == "TextLabel" and v.Name == "User" then
            v:Destroy()
        else
        end
    end
end

function Jump(func)
_G.e = func
while _G.e == true do 
game.Players.LocalPlayer.Character.Humanoid.Jump = true
wait()
game.Players.LocalPlayer.Character.Humanoid.Jump = false
end
end

function rejoin()
local ts = game:GetService("TeleportService")

local p = game:GetService("Players").LocalPlayer

 

ts:Teleport(game.PlaceId, p)
end



function anti()
local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
   vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
   wait(1)
   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)
print("AFK BYPASSER working")
end

Start()


